nextflow_process {
  name "Test Process MSNBASEXIC"
  script "../main.nf"
  process "MSNBASXIC"

  tag "local"
  tag "msnbasexic"

  test("basic test") {
    config "./nextflow.config"
    when {
      process {
        """
      input[0] = [
        [ id: 'test' ],
        file("${projectDir}/modules/local/msnbasexic/tests/data/input/20240315_QCN1_001_03_New_STD.mzML", checkIfExists: true),
        file("${projectDir}/modules/local/msnbasexic/tests/data/input/qcn1.tsv", checkIfExists: true)
      ]
      input[1] =  "m3C"
      input[2] =  150
      input[3] =  20
      input[4] =  2
      input[5] =  false
      input[6] =  false
      input[7] =  "xic_plot.pdf"
      input[8] =  true
      """
      }
    }

    then {
      assertAll(
                { assert process.success},
                { assert snapshot(
                  file(process.out.xic_output).name,
                  process.out.versions
              ).match()})
    }
  }
  
}